#网关服务IP
netgd_host = 127.0.0.1
#网关服务端口
netgd_port = 846

#客户端IP
centd_host = 127.0.0.1
#客户端端口
centd_port = 847

#日志文件名称
log_name = centd
log_folder = log/

#日志等级
log_level = 0

#素材文件夹路径
data_folder = "SERVER_ROOT/template/"

#地图文件夹
map_folder = "SERVER_ROOT/maps/"

#脚本文件夹
script_folder = "SERVER_ROOT/scripts/"

#配置文件夹
etc_folder = "SERVER_ROOT/etc/"

#bin文件夹
bin_folder = "SERVER_ROOT/bin/"

#是否进程守护
process_method = "N"
